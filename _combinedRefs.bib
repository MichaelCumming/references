@inproceedings{williams2006exploring,
	Abstract = {JP:
This paper presents Damage, a smart bracelet used to alert the wearer to changes in a social network as well as to send and receive simple messages to other users. The interface consists of six LEDs connected to five snaps. The first LED (not attached to a snap closure) pulses white light if there is a message waiting for the user on the social network, while the other lights are linked to other members of the social network. Users can send each other messages in the form of lighting red, green or blue LEDs by opening or closing the snap. Some interesting points about this project include the concept that users can assign meaning to particular patterns of LEDs as opposed to a system where the meanings of patterns are predetermined. As the authors point out (5), this adds a layer of intimacy to the product such that only members of the group are aware of the meanings of the messages, and strangers may assume that the LEDs are decorative. With regards to form-factor, younger participants reviewing a sketch of the design were concerned that the flashing LEDs might be inappropriate for school (5) and would prefer something resembling a watch. The design was developed based on the observation that participants reviewing the researchers' earlier social network project found the vibration-based alerts frustrating when they came at inopportune times, despite the fact that they enjoyed being always connected to the social network (5).},
	Author = {Williams, Amanda and Farnham, Shelly and Counts, Scott},
	Booktitle = {CHI'06 extended abstracts on Human factors in computing systems},
	Date-Added = {2014-10-08 19:12:06 +0000},
	Date-Modified = {2014-10-08 19:12:06 +0000},
	Organization = {ACM},
	Pages = {1529--1534},
	Title = {Exploring wearable ambient displays for social awareness},
	Year = {2006}}

@inproceedings{toney2002minimal,
	Abstract = {JP:
