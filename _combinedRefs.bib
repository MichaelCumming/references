@article{sallnas2000supporting,
	Abstract = {JP:
This paper documents a study on whether haptic feedback influences perceived social presence, perceived virtual presence, perceived task completion efficacy, and/or task completion efficacy (measured by time it takes to perform a task). In the study, users performed a set of tasks with the PHANToM device: an interface used for remotely performing surgery. Perceived virtual presence, perceived task completion efficacy and task completion efficacy were all highest when a combination of haptic, audio and visual feedback was used as compared to just audio and visual feedback.  },
